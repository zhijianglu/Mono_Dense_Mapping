<launch>

<arg name="config_path" default = "$(find feature_tracker)/../config/euroc/euroc_MH5_config.yaml" />

  <node pkg="open_quadtree_mapping" type="open_quadtree_mapping_node" name="open_quadtree_mapping" clear_params="true" output="screen">

    <param name="config_file" type="string" value="$(arg config_path)" />


    <!--960x540 dji dataset-->
    <!--<param name="cam_width" value="960" />-->
    <!--<param name="cam_height" value="540" />-->

    <!--<param name="cam_fx" value="670.598859798134" />-->
    <!--<param name="cam_fy" value="671.530674182169" />-->
    <!--<param name="cam_cx" value="481.527318331448" />-->
    <!--<param name="cam_cy" value="272.191272193949" />-->

    <!--<param name="cam_k1" value="0.0124895471802617" />-->
    <!--<param name="cam_k2" value="-0.0401120223191336" />-->
    <!--<param name="cam_r1" value="0.00149720377396837" />-->
    <!--<param name="cam_r2" value="0.0429412541158549" />-->


    <!--768x432 dji dataset-->
    <param name="cam_width" value="768" />
    <param name="cam_height" value="432" />

    <param name="cam_fx" value="537.210440680085" />
    <param name="cam_fy" value="538.06858705306" />
    <param name="cam_cx" value="385.668101634721" />
    <param name="cam_cy" value="217.56531220143" />

    <param name="cam_k1" value="0.0167283856029527" />
    <param name="cam_k2" value="-0.0489134846557019" />
    <param name="cam_r1" value="0.00184608847842459" />
    <param name="cam_r2" value="0.0438925745834179" />


    <param name="downsample_factor" value="1"/>
    <param name="semi2dense_ratio" value="3"/>

    <!--将vins输出的pose的topic映射成自己的topic image和posestamped， 方便测试不同数据集-->
    <remap from="~image" to="/dji_data/image_raw" />
    <remap from="~posestamped" to="/orb_slam2_mono/pose" />
  </node>
    <node name="rvizvisualisation" pkg="rviz" type="rviz" output="log" args="-d $(find open_quadtree_mapping)/launch/MH5_Globalmapping_config.rviz" />

</launch>
